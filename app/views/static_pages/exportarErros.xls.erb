<% @pacients.each do |pacient| %>
    <% count = 0 %>
    <Row>
      <Cell><Data ss:Type="Number"><%= pacient.id_amostra %></Data></Cell>
      <Cell><Data ss:Type="String"><%= pacient.nome %></Data></Cell>
      <Cell><Data ss:Type="String"><%= pacient.dataNasc %></Data></Cell>
      <Cell><Data ss:Type="String"><%= pacient.prontuario %></Data></Cell>
      <Cell><Data ss:Type="String"><%= pacient.genero %></Data></Cell>

      <%# @variables[0] = Genotipagem.find_all_by_paciente_id(pacient.id) %>
      <% geno = Genotipagemerro.find_by_paciente_id(pacient.id) %>

      <Cell><Data ss:Type="String"><%=geno.localProcedencia%></Data></Cell>
      <Cell><Data ss:Type="String"><%=geno.dataColeta%></Data></Cell>
      <Cell><Data ss:Type="String"><%=geno.dataRecep%></Data></Cell>

      <% @variables[1] = Esquemaerro.find_all_by_paciente_id(pacient.id).last(10) %>
      <% @variables[1].each do |esq| %>
          <Cell><Data ss:Type="String"><%= esq.DataIni %></Data></Cell>
          <% if esq.atual.eql?('N') %>
              <Cell><Data ss:Type="String"><%= esq.DataFim %></Data></Cell>
          <% else %>
              <Cell><Data ss:Type="String">Atual</Data></Cell>
          <% end %>
          <% med = Medicamentoerro.all(:joins => 'inner join esquemaerros_medicamentoerros on medicamentoerros.id = medicamentoerro_id inner join esquemaerros on esquemaerros.id = esquemaerro_id', :conditions => 'esquemaerro_id = '+esq.id.to_s) %>
          <% string = String.new("") %>
          <% med.each do |m| %>
              <% string << m.abreviacao + " + " %>
          <% end %>
          <Cell><Data ss:Type="String"><%= string.rstrip.chop %></Data></Cell>
          <% count += 1 %>
      <% end %>
      <%#MUTACOES %>
      <% while count < 10 %>
          <Cell><Data ss:Type="String"></Data></Cell>
          <Cell><Data ss:Type="String"></Data></Cell>
          <Cell><Data ss:Type="String"></Data></Cell>
          <% count += 1 %>
      <% end %>
      <% mp=[] , poli=[] , itrn = [] , itrnn = [], politr = [] %>
      <% mp = [] %>
      <% mutacoes = Mutacaoerro.all(:joins => 'inner join genotipagemerros_mutacaoerros on mutacaoerros.id = mutacaoerro_id inner join genotipagemerros on genotipagemerros.id = genotipagemerro_id', :conditions => 'genotipagemerro_id = '+geno.id.to_s) %>
      <% mutacoes.each do |m| %>
          <% if m.regiao == "Mutacoes principais" %>
              <% mp.push(m) %>
          <% end %>
          <% if m.regiao == "Polimorfismos"  %>
              <% poli<<m %>
          <% end %>
          <% if m.regiao == "ITRN" %>
              <% itrn<<m %>
          <% end %>
          <% if m.regiao == "ITRNN"  %>
              <% itrnn<<m %>
          <% end %>
          <% if m.regiao == "Polimorfismo na TR" %>
              <% politr<<m %>
          <% end %>
      <% end %>
      <% string = String.new("") %>
      <% mp.each { |x| string << x.sigla + " "} %>
      <Cell><Data ss:Type="String"><%= string %></Data></Cell>
      <% string = String.new("") %>
      <% poli.each { |x| string << x.sigla + " "} %>
      <Cell><Data ss:Type="String"><%= string %></Data></Cell>
      <% string = String.new("") %>
      <% itrn.each { |x| string << x.sigla + " "} %>
      <Cell><Data ss:Type="String"><%= string %></Data></Cell>
      <% string = String.new("") %>
      <% itrnn.each { |x| string << x.sigla + " "} %>
      <Cell><Data ss:Type="String"><%= string %></Data></Cell>
      <% string = String.new("") %>
      <% politr.each { |x| string << x.sigla + " "} %>
      <Cell><Data ss:Type="String"><%= string %></Data></Cell>

      <% exame = Exameerro.all(:conditions => "tipo = 'CV' AND paciente_id = "+pacient.id.to_s).last(3) %>
      <% i=0 %>
      <% exame.each do |ex| %>
          <% if ex.sinal.eql?('>') || ex.sinal.eql?('<') %>
            <Cell><Data ss:Type="String"><%= ex.sinal+ex.valor.to_s %></Data></Cell>
            <Cell><Data ss:Type="String"><%= Math.log(ex.valor,10) %></Data></Cell>
            <Cell><Data ss:Type="String"><%= ex.data %></Data></Cell>
          <% else %>
            <Cell><Data ss:Type="String"><%= ex.valor %></Data></Cell>
            <Cell><Data ss:Type="String"><%= Math.log(ex.valor,10) %></Data></Cell>
            <Cell><Data ss:Type="String"><%= ex.data %></Data></Cell>
          <% end %>
          <% i += 1%>
      <% end %>
      <% if i<3 %>
          <% while i<3 %>
              <Cell><Data ss:Type="String"></Data></Cell>
              <Cell><Data ss:Type="String"></Data></Cell>
              <Cell><Data ss:Type="String"></Data></Cell>
              <% i += 1%>
          <% end %>
      <% end %>

      <% exame = Exameerro.all(:conditions => "tipo = 'CD4' AND paciente_id = "+pacient.id.to_s).last(4)    %>
      <% exame.each do |ex| %>
          <% if ex.nil? %>
              <Cell><Data ss:Type="String"></Data></Cell>
              <Cell><Data ss:Type="String"></Data></Cell>
          <% else %>
              <% if ex.sinal.eql?('>') || ex.sinal.eql?('<') %>
                  <Cell><Data ss:Type="String"><%= ex.sinal+ex.valor.to_s %></Data></Cell>
                  <Cell><Data ss:Type="String"><%= ex.data %></Data></Cell>
              <% else %>
                  <Cell><Data ss:Type="String"><%= ex.valor %></Data></Cell>
                  <Cell><Data ss:Type="String"><%= ex.data %></Data></Cell>
              <% end %>
          <% end %>
      <% end %>

    </Row>
<% end %>
</Table>
