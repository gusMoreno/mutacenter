  <Cell ss:StyleID="BorderB"><Data ss:Type="String">data</Data></Cell>
  <Cell ss:StyleID="BorderB"><Data ss:Type="String">mais baixo esquema atual</Data></Cell>
  <Cell ss:StyleID="BorderB"><Data ss:Type="String">log</Data></Cell>
  <Cell ss:StyleID="BorderB"><Data ss:Type="String">data</Data></Cell>
